{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ],
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "one-var": "off"
  },
  "extends": "airbnb"
}